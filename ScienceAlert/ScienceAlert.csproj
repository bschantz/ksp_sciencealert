  <PropertyGroup>
    <PostBuildEvent>

echo ------------------ Installing %25ProjectName%25 ---------------------

call "$(SolutionDir)SetEnvironment.bat"

set ProjectGameData=$(ProjectDir)GameData\$(ProjectName)\


call:CreateSymbolsForDependencies

if $(Configuration) == Release (
echo Merging dependencies for release build...
call:MergeDependencies
) else (
echo Copying dependencies
REM call:CopyDependencies
)

call "$(SolutionDir)CopyToLocation.bat" "$(TargetDir)" "%25ProjectGameData%25" $(TargetName).* /xf *.config *.pdb

REM ----------------------------------------------------------
REM Create debug symbols for ScienceAlert
REM ----------------------------------------------------------
echo Creating debug symbols for $(TargetFileName)
echo Symbol destination: %25ProjectGameData%25
cd "$(TargetDir)"
call "$(SolutionDir)CreateMdb.bat" "$(TargetFileName)" "%25ProjectGameData%25"


cd "%25ProjectGameData%25"
if exist "%25ProjectGameData%25$(TargetName).reloadable" (
del "%25ProjectGameData%25$(TargetName).reloadable"

if exist "%25ProjectGameData%25$(TargetName).reloadable.mdb" (
del "%25ProjectGameData%25$(TargetName).reloadable.mdb"
)
)
call:RenameAssemblyToReloadable $(TargetName)
goto:CopyIntoGameDir


REM ----------------------------------------------------------
REM Copy dependencies into Project GameData
REM ----------------------------------------------------------
:CopyDependencies
echo copy dependencies to project GameData...
call "$(SolutionDir)CopyToLocation.bat" "$(TargetDir)" "%25ProjectGameData%25" ReeperCommon.* /xf *.pdb
call "$(SolutionDir)CopyToLocation.bat" "$(TargetDir)" "%25ProjectGameData%25" StrangeIoC.* /xf *.pdb
goto:eof


REM ----------------------------------------------------------
REM Merge dependencies into ScienceAlert.dll (used in release builds)
REM ----------------------------------------------------------
:MergeDependencies
echo merge dependencies...
cd "(TargetDir)"
"$(SolutionDir)packages\ILRepack.2.0.4\tools\ILRepack.exe" /verbose /out:$(TargetFileName) $(TargetFileName) ReeperCommon.dll StrangeIoC.dll

goto:eof


REM ----------------------------------------------------------
REM Create debug symbols for dependencies
REM ----------------------------------------------------------
:CreateSymbolsForDependencies
echo Creating symbols for dependencies...
cd "%25TargetDir%25"
call "$(SolutionDir)CreateMdb.bat" "$(TargetDir)ReeperCommon.dll" "$(TargetDir)"
call "$(SolutionDir)CreateMdb.bat" "$(TargetDir)StrangeIoC.dll" "$(TargetDir)"
goto:eof


REM ----------------------------------------------------------
REM Rename DLL to RELOADABLE so AssemblyReloader will be used to load it 
REM ----------------------------------------------------------
:RenameAssemblyToReloadable
echo renaming %25~1.dll to %25~1.reloadable...
ren %25~1.dll *.reloadable
if EXIST "%25~1.dll.mdb" (
echo renaming %25~1.dll.mdb to %25~1.reloadable.mdb...
ren %25~1.dll.mdb *.
ren %25~1.dll *.reloadable.mdb
)
echo Done!
goto:eof


REM ----------------------------------------------------------
REM Copy Project GameData into KSP Project GameData
REM ----------------------------------------------------------
:CopyIntoGameDir
REM Solution GameData to KSP GameData

echo Copying to GameData
call "$(SolutionDir)CopyToLocation.bat" "%25ProjectGameData%25" "%25ksp%25$(ProjectName)" /E /xo


echo Files copied to GameData successfully


REM return with no error; robocopy success will set ERRORLEVEL nonzero which will trick VS into thinking the build failed
exit 0</PostBuildEvent>
